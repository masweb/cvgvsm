{
  "name": "cvgvsm",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix --cache",
    "lint": "run-s lint:*",
    "format": "prettier --write --experimental-cli src/",
    "deploy": "cross-env NODE_ENV=production VITE_APP_BASE_URL=/ npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@coreui/coreui": "^5.4.3",
    "@coreui/vue": "^5.7.0",
    "@tabler/icons-vue": "^3.36.0",
    "@vueuse/core": "^14.1.0",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vee-validate": "^4.15.1",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.5",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "~1.29.0",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.29.0",
    "prettier": "3.6.2",
    "sass-embedded": "^1.97.0",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^29.2.0",
    "vite": "^7.3.0",
    "vue-tsc": "^3.1.8"
  }
}
