{
  "name": "cvgvsm",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "deploy": "cross-env NODE_ENV=production VITE_APP_BASE_URL=/cvgvsm/ npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@tabler/icons": "^3.22.0",
    "@vueuse/core": "^10.11.1",
    "basscss": "^8.1.0",
    "gh-pages": "^6.2.0",
    "naive-ui": "^2.40.1",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "vue-tabler-icons": "^2.21.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.6",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.31.0",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "typescript": "~5.5.4",
    "vite": "^5.4.11",
    "vue-tsc": "^2.1.10"
  }
}
