{
  "name": "cvgvsm",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "deploy": "cross-env NODE_ENV=production VUE_APP_BASE_URL=/cvgvsm/ npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@tabler/icons": "^3.4.0",
    "@vueuse/core": "^10.9.0",
    "basscss": "^8.1.0",
    "gh-pages": "^6.1.1",
    "naive-ui": "^2.38.2",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "vue-tabler-icons": "^2.21.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vue-tsc": "^2.0.19"
  }
